<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Actors.Electoral.ElectoralFragment">


    <Button
        android:id="@+id/BlockchainPanelBtn"
        android:layout_width="163dp"
        android:layout_height="90dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="120dp"
        android:text="Start blockchain"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

    <Button
        android:id="@+id/CandiatePanelBtn"
        android:layout_width="194dp"
        android:layout_height="64dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="140dp"
        android:layout_marginEnd="99dp"
        android:text="List of Candidates"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/BlockchainPanelBtn" />
    />

</androidx.constraintlayout.widget.ConstraintLayout>