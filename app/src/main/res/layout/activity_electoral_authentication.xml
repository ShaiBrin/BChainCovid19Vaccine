<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/ElectoralAuthenticationView"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.Activities.ElectoralAuthentication">

    <EditText
        android:id="@+id/ElectoralAuthentication"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="98dp"
        android:layout_marginTop="244dp"
        android:layout_marginEnd="103dp"
        android:ems="10"
        android:hint="Electoral Key"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ElectoralKeyBtn"
        android:layout_width="109dp"
        android:layout_height="64dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="151dp"
        android:text="Enter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ElectoralAuthentication" />

</androidx.constraintlayout.widget.ConstraintLayout>